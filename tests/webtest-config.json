{
  "mozsearch_path": "$MOZSEARCH_PATH",
  "config_repo": "$CONFIG_REPO",

  "default_tree": "tests",

  "trees": {
    "tests": {
      "IMPORT": {
        "file": "$MOZSEARCH_PATH/tests/config.json",
        "tree_name": "tests"
      }
    },
    "searchfox": {
      "IMPORT": {
        "file": "$MOZSEARCH_PATH/tests/searchfox-config.json",
        "tree_name": "searchfox"
      }
    }
  },

  "allow_webtest": true
}
